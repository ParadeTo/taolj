#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/fork-ts-checker-webpack-plugin@7.2.14_typescript@4.9.4+webpack@5.75.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/fork-ts-checker-webpack-plugin@7.2.14_typescript@4.9.4+webpack@5.75.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/fork-ts-checker-webpack-plugin@7.2.14_typescript@4.9.4+webpack@5.75.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/fork-ts-checker-webpack-plugin@7.2.14_typescript@4.9.4+webpack@5.75.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.9.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.9.4/node_modules/typescript/bin/tsc" "$@"
fi
