#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules/@angular-devkit/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.0.4_chokidar@3.5.3/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/schematics.js" "$@"
else
  exec node  "$basedir/../../bin/schematics.js" "$@"
fi
