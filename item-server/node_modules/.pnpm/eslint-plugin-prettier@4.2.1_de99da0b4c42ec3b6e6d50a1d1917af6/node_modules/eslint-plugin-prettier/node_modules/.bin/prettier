#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/prettier@2.8.1/node_modules/prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/prettier@2.8.1/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_de99da0b4c42ec3b6e6d50a1d1917af6/node_modules/eslint-plugin-prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_de99da0b4c42ec3b6e6d50a1d1917af6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/prettier@2.8.1/node_modules/prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/prettier@2.8.1/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_de99da0b4c42ec3b6e6d50a1d1917af6/node_modules/eslint-plugin-prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_de99da0b4c42ec3b6e6d50a1d1917af6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.8.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.8.1/node_modules/prettier/bin-prettier.js" "$@"
fi
