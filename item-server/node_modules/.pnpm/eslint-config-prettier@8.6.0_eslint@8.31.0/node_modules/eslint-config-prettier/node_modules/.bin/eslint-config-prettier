#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules/eslint-config-prettier/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules/eslint-config-prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules/eslint-config-prettier/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules/eslint-config-prettier/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint-config-prettier@8.6.0_eslint@8.31.0/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
