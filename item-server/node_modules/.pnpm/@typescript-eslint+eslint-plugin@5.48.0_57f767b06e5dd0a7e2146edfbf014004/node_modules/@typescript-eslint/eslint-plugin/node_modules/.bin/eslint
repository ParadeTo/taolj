#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules/eslint/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules/eslint/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules/@typescript-eslint/eslint-plugin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules/@typescript-eslint/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules/eslint/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules/eslint/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/eslint@8.31.0/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules/@typescript-eslint/eslint-plugin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules/@typescript-eslint/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.48.0_57f767b06e5dd0a7e2146edfbf014004/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.31.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.31.0/node_modules/eslint/bin/eslint.js" "$@"
fi
