#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules/@nestjs/schematics/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules/@nestjs/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules/@nestjs/schematics/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules/@nestjs/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/@nestjs+schematics@9.0.4_chokidar@3.5.3+typescript@4.9.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.9.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.9.4/node_modules/typescript/bin/tsserver" "$@"
fi
