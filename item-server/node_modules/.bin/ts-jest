#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/ts-jest@29.0.3_jest@29.3.1+typescript@4.9.4/node_modules/ts-jest/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/ts-jest@29.0.3_jest@29.3.1+typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/ts-jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/ts-jest@29.0.3_jest@29.3.1+typescript@4.9.4/node_modules/ts-jest/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/ts-jest@29.0.3_jest@29.3.1+typescript@4.9.4/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/item-server/node_modules/ts-jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
