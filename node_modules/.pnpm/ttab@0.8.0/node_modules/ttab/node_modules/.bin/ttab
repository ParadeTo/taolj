#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules/ttab/bin/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules/ttab/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules/ttab/bin/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules/ttab/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/ttab@0.8.0/node_modules:/Users/youxingzhi/ayou/taolj/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../bin/ttab" "$@"
else
  exec bash  "$basedir/../../bin/ttab" "$@"
fi
