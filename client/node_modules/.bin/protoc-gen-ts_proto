#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/ts-proto@1.138.0/node_modules/ts-proto/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/ts-proto@1.138.0/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/ts-proto/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/ts-proto@1.138.0/node_modules/ts-proto/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/ts-proto@1.138.0/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/ts-proto/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-proto/protoc-gen-ts_proto" "$@"
else
  exec node  "$basedir/../ts-proto/protoc-gen-ts_proto" "$@"
fi
