#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules/protobufjs/bin/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules/protobufjs/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules/protobufjs/bin/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules/protobufjs/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/protobufjs@6.11.3/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules/.pnpm/node_modules:/Users/youxingzhi/ayou/taolj/client/node_modules:/Users/youxingzhi/ayou/taolj/node_modules:/Users/youxingzhi/ayou/node_modules:/Users/youxingzhi/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pbts" "$@"
else
  exec node  "$basedir/../../bin/pbts" "$@"
fi
